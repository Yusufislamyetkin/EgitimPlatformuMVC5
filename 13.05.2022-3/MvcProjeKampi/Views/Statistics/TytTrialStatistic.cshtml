
@{
    ViewBag.Title = "TytTrialStatistic";
    Layout = "~/Views/Shared/_StudentHome.cshtml";
}

<title>TYT Deneme İstatistikleri</title>
<div style="background-color: white; border-radius: 5px; margin-top: 30px; margin-left:60px; margin-right:100px ">
    <p style="text-align: center; font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans- serif; font-size: 20px; color: #8789FF;margin-top:14px; ">
        TYT Deneme İstatistikleri
    </p>
</div>
<link rel="stylesheet" href="~/Web2/assets/vendor/css/core.css" class="template-customizer-core-css" />
<link rel="stylesheet" href="~/Web2/assets/vendor/css/theme-default.css" class="template-customizer-theme-css" />
<script src="~/Web2/assets/vendor/js/bootstrap.js"></script>


<div style="margin-left:60px;margin-right:100px;margin-top:15px;">
    <div class="alert alert-primary alert-dismissible" role="alert">
        Deneme Genel İstatistik
    </div>
</div>
<div class="row" style="margin-top:30px; margin-left:50px;margin-right:50px;">

    <div class="col-lg-6 col-md-12 col-6 mb-4" style="width:282px;height:200px;">
        <div class="card">
            <div class="card-body">
                <div class="card-title d-flex align-items-start justify-content-between">
                    <div class="avatar flex-shrink-0">
                        <img src="~/Extra/elements/net.png"
                             alt="chart success"
                             class="rounded" />
                    </div>

                </div>
                <span class="fw-semibold d-block mb-1">Çözülen Deneme Sayısı</span>

                <div style="background-color: #E3F8D7; width: 215px; height: 40px; margin-right: 10px;">
                    <p style="text-align:center;font-size:24px;">  &#8594; @ViewBag.trialcount</p>
                </div>
            </div>
        </div>
    </div>

    <div class="col-lg-6 col-md-12 col-6 mb-4" style="width:282px;height:200px;">
        <div class="card">
            <div class="card-body">
                <div class="card-title d-flex align-items-start justify-content-between">
                    <div class="avatar flex-shrink-0">
                        <img src="~/Extra/elements/netsayisi.png"
                             alt="chart success"
                             class="rounded" />
                    </div>

                </div>
                <span class="fw-semibold d-block mb-1">En Yüksek Net</span>


                <div style="background-color: #E3F8D7; width: 215px; height: 40px; margin-right: 10px;">
                    <p style="text-align:center;font-size:24px;"> &#8594; @ViewBag.netlist</p>
                </div>
            </div>
        </div>
    </div>


    <div class="col-lg-6 col-md-12 col-6 mb-4" style="width:282px;height:200px;">
        <div class="card">
            <div class="card-body">
                <div class="card-title d-flex align-items-start justify-content-between">
                    <div class="avatar flex-shrink-0">
                        <img src="~/Extra/elements/puan.png"
                             alt="chart success"
                             class="rounded" />
                    </div>

                </div>
                <span class="fw-semibold d-block mb-1">En Yüksek Puan</span>


                <div style="background-color: #E3F8D7; width: 215px; height: 40px; margin-right: 10px;">
                    <p style="text-align:center;font-size:24px;"> &#8594;  @ViewBag.scorelist</p>
                </div>
            </div>
        </div>
    </div>

    <div class="col-lg-6 col-md-12 col-6 mb-4" style="width:282px;height:200px;">
        <div class="card">
            <div class="card-body">
                <div class="card-title d-flex align-items-start justify-content-between">
                    <div class="avatar flex-shrink-0">
                        <img src="~/Extra/elements/yayın.png"
                             alt="chart success"
                             class="rounded" />
                    </div>

                </div>
                <span class="fw-semibold d-block mb-1">Çözülen Farklı Yayın Sayısı</span>


                <div style="background-color: #E3F8D7; width: 215px; height: 40px; margin-right: 10px;">
                    <p style="text-align:center;font-size:24px;"> &#8594; @ViewBag.yayınevi</p>
                </div>
            </div>
        </div>
    </div>

</div>

@*Alert*@
