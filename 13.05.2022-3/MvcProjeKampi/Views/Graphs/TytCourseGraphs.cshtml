
@{
    ViewBag.Title = "TytCourseGraphs";
    Layout = "~/Views/Shared/_StudentHome.cshtml";
}
<link href="~/Extra/graphs1.css" rel="stylesheet" />
@{ string color = "";}
<div class="container mt-5">
    <div class="card">
        <div class="row no-gutters">
            <div class="col-md-4 border-right">
                <div class="ratings text-center p-4 py-5"> <span class="badge bg-success">TYT Konu Grafik <i class="fa fa-star-o"></i></span> <span class="d-block about-rating"></span> <span class="d-block total-ratings"></span> </div>
            </div>
            <div class="col-md-8">
                <div class="rating-progress-bars p-3">
                    <div class="progress-1 align-items-center">

                        <label style="font-weight:bold">Tyt Türkçe</label>
                        @if (@ViewBag.tytstaticticturk > 80)
                        {
                            color = "bg-success";
                        }
                        else if (@ViewBag.tytstaticticturk > 60)
                        {
                            color = "bg-primary";
                        }
                        else if (@ViewBag.tytstaticticturk > 30)
                        {
                            color = "bg-warning";
                        }
                        else if (@ViewBag.tytstaticticturk > 0)
                        {
                            color = "bg-danger";
                        }



                        <div class="progress">
                            <div class="progress-bar @color" role="progressbar" style="width:  @ViewBag.tytstaticticturk%;" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100">  @ViewBag.tytstaticticturk  </div>
                        </div>

                        <label style="font-weight:bold">Tyt Tarih</label>

                        @if (@ViewBag.tytstatictictar > 80)
                        {
                            color = "bg-success";
                        }
                        else if (@ViewBag.tytstatictictar > 60)
