                        </div>

                        <label style="font-weight:bold">Tyt Matematik</label>
                        @if (@ViewBag.tytstaticticmat > 80)
                        {
                            color = "bg-success";
                        }
                        else if (@ViewBag.tytstaticticmat > 60)
                        {
                            color = "bg-primary";
                        }
                        else if (@ViewBag.tytstaticticmat > 30)
                        {
                            color = "bg-warning";
                        }
                        else if (@ViewBag.tytstaticticmat > 0)
                        {
