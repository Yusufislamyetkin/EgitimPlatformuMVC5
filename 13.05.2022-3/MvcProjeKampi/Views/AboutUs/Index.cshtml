@model EntityLayer.Concrete.AboutUs
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_StudentHome.cshtml";
}

<script src="~/Extra/ContactFormjs.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<br />
<div class="container mt-5 mb-5 d-flex justify-content-center">
    <br />
    <br />
    <br />
    <div class="card rounded">
        <div class=" d-block justify-content-center">
            <div class="area1 p-3 py-5"> </div>
            <div class="area2 p- text-center px-3">
                <div class="image mr-3">
                    <img src="~/Extra/logo.png" class="rounded-circle" width="100" />
                    <h4 class=" name mt-3 ">Extrace</h4>
                    <p class="information mt-3 text-justify"> @Model.Content </p>
                    <div class="d-flex justify-content-center mt-5">
                        <ul class="list-icons">
                          
                          
                            <li class="facebook"><a href="@Model.facebooklink"><i class="fa fa-facebook"></i></a></li>
                            <li class="instagram"><a href="@Model.instagramlink"><i class="fa fa-instagram"></i></a></li>
                            <li class="whatsapp"><a href="@Model.whatsapplink"><i class="fa fa-whatsapp"></i></a></li>
                            <li class="pinterest"><a href="@Model.pinterestlink"><i class="fa fa-pinterest"></i></a></li>
                            <li class="youtube"><a href="@Model.youtubelink"><i class="fa fa-youtube"></i></a></li>
                           
                        </ul>
                    </div>
                </div>
                <div> </div>
            </div>
        </div>
    </div>
</div>